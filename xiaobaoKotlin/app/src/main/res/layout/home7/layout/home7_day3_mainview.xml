<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">



    <Button
        android:id="@+id/home7_day3_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="89dp"
        android:onClick="move"
        android:text="@string/click1"
        android:textAllCaps="false" />

    <Button
        android:id="@id/home7_day3_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/home7_day3_button1"
        android:layout_alignBottom="@+id/home7_day3_button1"
        android:layout_centerHorizontal="true"
        android:onClick="move"
        android:text="@string/click2"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/home7_day3_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/home7_day3_button2"
        android:layout_alignBottom="@+id/home7_day3_button2"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_toEndOf="@+id/home7_day3_button2"
        android:layout_toRightOf="@+id/home7_day3_button2"
        android:onClick="move"
        android:text="@string/click3"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/home7_day3_button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/home7_day3_button1"
        android:layout_alignStart="@+id/home7_day3_button1"
        android:layout_below="@+id/home7_day3_button1"
        android:layout_marginTop="31dp"
        android:onClick="move"
        android:text="@string/click4"
        android:textAllCaps="false" />


    <Button
        android:id="@+id/home7_day3_button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/home7_day3_button4"
        android:layout_alignBottom="@+id/home7_day3_button4"
        android:layout_alignLeft="@+id/home7_day3_button2"
        android:layout_alignStart="@+id/home7_day3_button2"
        android:onClick="move"
        android:text="@string/click5"
        android:textAllCaps="false" />


    <Button
        android:id="@+id/home7_day3_button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/home7_day3_button5"
        android:layout_alignBottom="@+id/home7_day3_button5"
        android:layout_alignLeft="@+id/home7_day3_button3"
        android:layout_alignStart="@+id/home7_day3_button3"
        android:onClick="move"
        android:text="@string/click6"
        android:textAllCaps="false" />




</RelativeLayout>
